                <tr>
                    <th><p><a class="btn btn-primary" href="@Url.Action("ListOfTenent","Tenent")">List of all the tenents &raquo;</a></p></th>
                    <th><p><a class="btn btn-success" href="@Url.Action("Create","Tenent")">Create a new tenent &raquo;</a></p></th>

                </tr>
            </table>

        </div>

    </div>
    <hr />

    <div class="row">

        <div class="col-md-push-12">
            <h2>Building Owners Account:</h2>
            <table style="width:100%">
                <tr>
                    <th><p><a class="btn btn-primary" href="@Url.Action("ListOfOwners","Owner")">List Building Owners &raquo;</a></p></th>
                    <th><p><a class="btn btn-success" href="@Url.Action("Create","Owner")">Create a new Owner &raquo;</a></p></th>

                </tr>
            </table>

        </div>

    </div>
    <hr />

    <div class="row">

        <div class="col-md-push-12">
            <h2>Buildings:</h2>
            <table style="width:100%">
                <tr>
                    <th><p><a class="btn btn-primary" href="@Url.Action("Index","Buildings")">List of all the Buildings &raquo;</a></p></th>
                    <th><p><a class="btn btn-success" href="@Url.Action("Create","Buildings")">Create a new Building &raquo;</a></p></th>

                </tr>
            </table>

        </div>

    </div>
    <hr />
    <div class="row">


            <table style="width:100%">
                <tr>
                    <th><p><a class="btn btn-primary" href="@Url.Action("Index","Messages")">List of all the Messages &raquo;</a></p></th>
                    <th><p><a class="btn btn-success" href="@Url.Action("Create","Messages")">Create a new Message &raquo;</a></p></th>


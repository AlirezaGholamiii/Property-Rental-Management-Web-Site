                <tr>
                    <th><p><a class="btn btn-primary" href="@Url.Action("Index","Apartments")">List of all the Apartments &raquo;</a></p></th>
                    <th><p><a class="btn btn-success" href="@Url.Action("Create","Apartments")">Create a new Apartment &raquo;</a></p></th>

                </tr>
            </table>

        </div>

    </div>

    <div class="row">

        <div class="col-md-push-12">
